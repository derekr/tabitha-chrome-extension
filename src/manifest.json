{
    "manifest_version": 2,

    "name": "Tab Champ",
    "description": "CHDC",
    "version": "1.2",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Tab Champ!"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "permissions": [
        "tabs",
        "http://localhost/*",
        "https://*/*",
        "http://*/*"
    ]
}
