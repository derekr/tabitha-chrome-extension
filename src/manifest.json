{
    "manifest_version": 2,

    "name": "Tab Queen",
    "description": "CHDC",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Hey!",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "permissions": [
        "tabs",
        "http://localhost/*",
        "https://*/*",
        "http://*/*"
    ]
}
